{
  "name": "panda/cart",
  "description": "Shopping cart",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Fozil Umarov",
      "email": "mr.foza@bk.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Panda\\Cart\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Panda\\Cart\\Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "illuminate/container": "^6.0|^7.0",
    "illuminate/database": "^6.0|^7.0",
    "illuminate/redis": "^6.0|^7.0",
    "illuminate/support": "^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.4|^9.0",
    "predis/predis": "^1.1"
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Panda\\Cart\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
